<module>

	<!-- Inherit the core Web Toolkit stuff.                  -->
	<inherits name='com.google.gwt.user.User'/>

	<!-- Specify the app entry point class.                   -->
	<entry-point class='edu.washington.apl.weather.gwt.client.GWTWeather'/>
	
	<!-- servlet for rpc calls -->
	<servlet path='/service' class='edu.washington.apl.weather.gwt.server.GWTWeatherServiceImpl'/>
</module>
